generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Climb {
  id        Int      @id
  climb     String
  grade     Int
  sessionId Int?
  Session   Session? @relation(fields: [sessionId], references: [id])
}

model Session {
  id    Int     @id
  name  String
  round Int
  time  Int
  Climb Climb[]
}
